    "gh-pages": "^3.0.0",
    "husky": "4.2.5",
    "jest": "26.4.0",
    "prettier": "2.0.5",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.9.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn format",
      "pre-push": "yarn test"
    }
  }
